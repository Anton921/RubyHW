<h1>Products</h1>

<div>
  <table>
    <tbody>
      <% @products.each do |products| %>
        <tr>
          <td>
            <dl>
              <dt> <%= products.title %> </dt>
              <dt> <%= products.created_at %> </dt>
            </dl>
          </td>
          <td>
            <%= image_tag("#{products.image}") %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>